# references:
# http://fahdshariff.blogspot.co.il/2011/04/writing-your-own-bash-completion.html

pdmt_complete()
{
	local cur=${COMP_WORDS[COMP_CWORD]}
	local list=$($HOME/git/branches/pdmt/test.py --printnodes)
	#echo "cur is " $cur
	#echo "list is " $list
	COMPREPLY=( $(compgen -W "$list" -- $cur) )
	#echo "COMPREPLY is " $COMPREPLY
}
complete -F pdmt_complete test.py 
